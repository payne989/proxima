package webservices;

import java.sql.SQLException;
import java.util.ArrayList;

import javax.jws.WebService;
import javax.naming.NamingException;

import dao.ContoDao;
import model.Conto;

@WebService
public class ContoWebService {

	public ArrayList<Conto> selectConto() throws SQLException, NamingException {

		return ContoDao.select();

	}

}
