package ejb;

import javax.ejb.LocalBean;
import javax.ejb.Stateless;

import dao.CorsoDao;

/**
 * Session Bean implementation class Corso_ejb
 */
@Stateless
@LocalBean
public class Corso_ejb implements Corso_ejbRemote, Corso_ejbLocal {

    
    public Corso_ejb() {
        
    }

    public boolean insertCorso (String nome) {
    	
    	System.out.println("Corso_ejb.insertCorso nome:"+nome);
    	
    	return CorsoDao.insertCorso(nome);
    	
    }
}
