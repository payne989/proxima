package model;

import javax.xml.datatype.XMLGregorianCalendar;

public class Edizione {

	private int id;
	private int idCorso;
	private int idSede;
	private int idDocente;
	private XMLGregorianCalendar dataIn;
	private XMLGregorianCalendar dataFin;
	
	public Edizione (int id, int idCorso, int idSede, int idDocente, XMLGregorianCalendar dataIn, XMLGregorianCalendar dataFin){
		
		this.id=id;
		this.idCorso=idCorso;
		this.idSede=idSede;
		this.idDocente=idDocente;
		this.dataIn=dataIn;
		this.dataFin=dataFin;
		}
	
	public Edizione () {}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getIdCorso() {
		return idCorso;
	}

	public void setIdCorso(int idCorso) {
		this.idCorso = idCorso;
	}

	public int getIdSede() {
		return idSede;
	}

	public void setIdSede(int idSede) {
		this.idSede = idSede;
	}

	public int getIdDocente() {
		return idDocente;
	}

	public void setIdDocente(int idDocente) {
		this.idDocente = idDocente;
	}

	public XMLGregorianCalendar getDataIn() {
		return dataIn;
	}

	public void setDataIn(XMLGregorianCalendar dataIn) {
		this.dataIn = dataIn;
	}

	public XMLGregorianCalendar getDataFin() {
		return dataFin;
	}

	public void setDataFin(XMLGregorianCalendar dataFin) {
		this.dataFin = dataFin;
	}
	
	
	
	
	
	
	
	
	
	
	
	
}
