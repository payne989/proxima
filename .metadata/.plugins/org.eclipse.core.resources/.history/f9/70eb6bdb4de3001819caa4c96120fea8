package ejb;

import javax.ejb.LocalBean;
import javax.ejb.Stateless;

import dao.DocenteDao;

/**
 * Session Bean implementation class Docente_ejb
 */
@Stateless
@LocalBean
public class Docente_ejb implements Docente_ejbRemote, Docente_ejbLocal {

   
    public Docente_ejb() {
        
    }

    public boolean insertDocente (String nome, String cognome, String cf){
		
    	return DocenteDao.insertDocente(nome, cognome, cf);
    	
    }
}
